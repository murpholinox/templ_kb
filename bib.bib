file = {:home/murphy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Terwilliger, Berendzen - 1999 - Evaluation of macromolecular electron-density map quality using the correlation of local r.m.s. density.pdf:pdf},
issn = {09074449},
journal = {Acta Crystallographica Section D: Biological Crystallography},
