file = {:home/murphy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lang et al. - 2014 - Protein structural ensembles are revealed by redefining X-ray electron density noise.pdf:pdf},
issn = {0027-8424},
