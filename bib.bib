issn = {0907-4449},
journal = {Acta Crystallographica Section D Biological Crystallography},
keywords = {Disulfide bonds,Electron paramagnetic resonance,Protein,Radiation damage,UV-visible absorption microspectrophotometry},
month = {dec},
number = {12},
pages = {2381--2394},
publisher = {International Union of Crystallography},
title = {{Insights into the mechanism of X-ray-induced disulfide-bond cleavage in lysozyme crystals based on EPR, optical absorption and X-ray diffraction studies}},
