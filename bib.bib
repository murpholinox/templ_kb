journal = {Journal of Synchrotron Radiation},
keywords = {Chemical degradation,Dose effects,Dose rate,Radiation damage,Scavengers},
title = {{Physical and chemical considerations of damage induced in protein crystals by synchrotron radiation: A radiation chemical perspective}},
year = {2002}
}
