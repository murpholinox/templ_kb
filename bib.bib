publisher = {Elsevier B.V.},
title = {{pH dependent protein stability: A quantitative approach based on Kramer's barrier escape}},
url = {http://dx.doi.org/10.1016/j.cplett.2014.11.002 https://linkinghub.elsevier.com/retrieve/pii/S0009261414009361},
