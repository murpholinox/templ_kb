file = {:home/murphy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Murray et al. - 2005 - Parameters affecting the X-ray dose absorbed by macromolecular crystals.pdf:pdf},
issn = {0909-0495},
journal = {Journal of Synchrotron Radiation},
keywords = {Absorbed dose,Dodecin,Henderson limit,Radiation damage,X-ray diffraction},
month = {may},
number = {3},
pages = {268--275},
title = {{Parameters affecting the X-ray dose absorbed by macromolecular crystals}},
