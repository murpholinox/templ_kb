file = {:home/murphy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Borek et al. - 2010 - Diffraction data analysis in the presence of radiation damage.pdf:pdf},
issn = {0907-4449},
journal = {Acta Crystallographica Section D Biological Crystallography},
keywords = {Experimental phasing,Radiation-induced specific changes,Relative B factor,Scaling B factor,Synchrotron radiation},
month = {apr},
number = {4},
pages = {426--436},
publisher = {International Union of Crystallography},
title = {{Diffraction data analysis in the presence of radiation damage}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0907444909040177},
volume = {66},
year = {2010}
