volume = {45},
year = {2017}
}
@article{Kantardjieff2004,
abstract = {A polemic in response to Huber and Kobe (Bioinformatics 22 Sept. 2004, 20(14), 2169-2170) is given. Kantardjieff and Rupp, although agreed to Huber and Kobe concerning their confusing presentation of the correlation of pI and pH, argued that the correlation reported erroneously was never used to produce a predictive model via regression anal., which was overlooked by Huber and Kobe. They emphasized that Huber and Kobe's statement regarding their conclusion is flawed, and that following an exptl. distribution suitable for the selected pI range to increase overall efficiency is completely valid. Furthermore, for the pI {\textless} 7 group, a correlation with crystn. pH has been established, which could be used to support a predictive model. Kantardjieff and Rupp mention that their statistical anal. suggests favorable pH distributions and ranges for improved efficiency of crystn. screening expts., validating early grid designs that varied pH around physiol. values. [on SciFinder (R)]},
author = {Kantardjieff, K. A. and Jamshidian, M. and Rupp, B.},
doi = {10.1093/bioinformatics/bth453},
issn = {1367-4803},
journal = {Bioinformatics},
title = {{Distributions of pI versus pH provide prior information for the design of crystallization screening experiments: response to comment on 'Protein isoelectric point as a predictor for increased crystallization screening efficiency'}},
year = {2004}
