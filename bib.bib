keywords = {SAXS,X-ray radiation damage,dose,imaging,macromolecular crystallography,scavengers},
month = {jan},
number = {1},
pages = {1--6},
title = {{X-ray radiation damage to biological macromolecules: further insights}},
url = {https://doi.org/10.1107/S160057751602018X},
volume = {24},
year = {2017}
}
@article{Katoh2012,
abstract = {Two methods to add unaligned sequences into an existing multiple sequence alignment have been implemented as the '-add' and '-addfragments' options in the MAFFT package. The former option is a basic one and applicable only to full-length sequences, whereas the latter option is applicable even when the unaligned sequences are short and fragmentary. These methods internally infer the phylogenetic relationship among the sequences in the existing alignment and the phylogenetic positions of unaligned sequences. Benchmarks based on two independent simulations consistently suggest that the "-addfragments" option outperforms recent methods, PaPaRa and PAGAN, in accuracy for difficult problems and that these three methods appropriately handle easy problems. {\textcopyright} The Author 2012. Published by Oxford University Press. All rights reserved.},
