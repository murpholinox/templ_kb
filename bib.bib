issn = {00796107},
journal = {Progress in Biophysics and Molecular Biology},
keywords = {Counter-diffusion crystallization,High-throughput crystallization,Southeast Collaboratory for Structural Genomics (S,Structural genomics,X-ray crystallography},
month = {jul},
number = {3},
pages = {359--386},
title = {{Life in the fast lane for protein crystallization and X-ray crystallography}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0079610704000987},
volume = {88},
year = {2005}
}
